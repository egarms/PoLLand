<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wisc.app"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk android:minSdkVersion="10" />

    <application
        android:icon="@drawable/pollandicon"
        android:label="@string/app_name" >
        <activity
            android:name=".PoLLandActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <activity android:name=".CreatePollActivity"
			android:label="Create Your Poll">
		</activity>
		<activity android:name=".createPollSettingsActivity"
			android:label="Poll Settings">
		</activity>
		<activity android:name=".PollDisplayActivity"
			android:label="Cast your vote!">
		</activity>
        
    </application>

</manifest>